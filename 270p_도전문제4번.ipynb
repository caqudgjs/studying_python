   "source": [
    "a = 1, 2, [3, 4], 5, [6, 7], [8, 9]\n",
    "output = []\n",
    "\n",
    "for i in a:\n",
    "    if type(i) == list:\n",
    "        for j in i:\n",
    "            output.append(j)\n",
    "    else:\n",
    "        output.append(i)\n",
    "\n",
    "print(f\"{a}를 평탄화 하면\")\n",
    "print(f\"{output}입니다.\")"
   ]
